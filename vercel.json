{
  "version": 2,
  "framework": "vite",
  "installCommand": "cd react-101 && npm ci",
  "buildCommand": "cd react-101 && npm run build",
  "outputDirectory": "react-101/dist",
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "github": {
    "silent": true
  }
}
